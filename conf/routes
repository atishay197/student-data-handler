GET     /                           controllers.Application.index
GET     /home                       controllers.Assets.at(path="/public/Html",file="MainView.html")
POST    /storeData                  controllers.Application.storeData
POST    /getData                    controllers.Application.getData
GET     /home/*roll                 controllers.Application.showRoll(roll)
POST    /submitGrades               controllers.Application.submitGrades()
GET     /showDetails                controllers.Application.showDetails()